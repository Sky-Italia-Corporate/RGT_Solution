                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- First Period (&lt;= X-6)
USE SKY_RGT
select 
MIN(Period_ID) as Period_ID,
'FIRST-PERIOD' as Period,
min(PeriodStart) as PeriodStartDate,
max(PeriodEnd) as PeriodEndDate,
min(FiscalWeek) as FiscalWeek,
min(FiscalMonth) as FiscalMonth,
min(Fyear) as FiscalYear,
min(Fweek) as Fweek,
min(Fmonth) as Fmonth,
min(CivilMonth) as CivilMonth,
min(CYear) as CivilYear,
min(Cmonth) as Cmonth,
cast(cast(min(CYear) as char(4))+right(cast(min(CivilMonth) as char(4)),2) as int) as CivilMonthYear_ID,
cast(cast(min(Fyear) as char(4))+right(cast(min(FiscalWeek) as char(3)),2) as int) as FiscalWeekYear_ID
from SQL02.[SKY_SA].dbo.[M_PERIOD_SOURCE]
where Fyear &lt;=
(
	select cast(SET_VALUE as int)-6
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
union all
--Tre anni a dettaglio annuale (da X-5 a X-2)
select 
MIN(Period_ID) as Period_ID,
'FY'+right(Fyear,2)+'-'+'CY'+RIGHT(cast(Cyear as char(4)),2)
as Period,
min(PeriodStart) as PeriodStartDate,
max(PeriodEnd) as PeriodEndDate,
min(FiscalWeek) as FiscalWeek,
min(FiscalMonth) as FiscalMonth,
min(Fyear) as FiscalYear,
min(Fweek) as Fweek,
min(Fmonth) as Fmonth,
min(CivilMonth) as CivilMonth,
min(CYear) as CivilYear,
min(Cmonth) as Cmonth,
cast(cast(min(CYear) as char(4))+right(cast(min(CivilMonth) as char(4)),2) as int) as CivilMonthYear_ID,
cast(cast(min(Fyear) as char(4))+right(cast(min(FiscalWeek) as char(3)),2) as int) as FiscalWeekYear_ID
from SQL02.[SKY_SA].dbo.[M_PERIOD_SOURCE]
where Fyear &gt;=
(
	select cast(SET_VALUE as int)-5
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
and Cyear &lt;=
(
	select cast(SET_VALUE as int)-2
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
and Fyear &lt;=
(
	select cast(SET_VALUE as int)-2
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
group by 
'FY'+right(Fyear,2)+'-'+'CY'+RIGHT(cast(Cyear as char(4)),2), Fyear
union all
--Tre anni a dettaglio settimanale (da X-1 a X+1)
select 
Period_ID,
PeriodCinemaPTV as Period,
PeriodStart as PeriodStartDate,
PeriodEnd as PeriodEndDate,
FiscalWeek as FiscalWeek,
FiscalMonth as FiscalMonth,
Fyear as FiscalYear,
Fweek as Fweek,
Fmonth as Fmonth,
CivilMonth as CivilMonth,
CYear as CivilYear,
Cmonth as Cmonth,
cast(cast(CYear as char(4))+right(cast(CivilMonth as char(4)),2) as int) as CivilMonthYear_ID,
cast(cast(Fyear as char(4))+right(cast(FiscalWeek as char(3)),2) as int) as FiscalWeekYear_ID
from SQL02.[SKY_SA].dbo.[M_PERIOD_SOURCE]
where Fyear &gt;=
(
	select cast(SET_VALUE as int)-1
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
and Fyear &lt;=
(
	select cast(SET_VALUE as int)+1
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
union				-- bug fix cambio anno FY12-FY13

select 
Period_ID,
PeriodCinemaPTV as Period,
PeriodStart as PeriodStartDate,
PeriodEnd as PeriodEndDate,
FiscalWeek as FiscalWeek,
FiscalMonth as FiscalMonth,
Fyear as FiscalYear,
Fweek as Fweek,
Fmonth as Fmonth,
CivilMonth as CivilMonth,
CYear as CivilYear,
Cmonth as Cmonth,
cast(cast(CYear as char(4))+right(cast(CivilMonth as char(4)),2) as int) as CivilMonthYear_ID,
cast(cast(Fyear as char(4))+right(cast(FiscalWeek as char(3)),2) as int) as FiscalWeekYear_ID
from SQL02.[SKY_SA].dbo.[M_PERIOD_SOURCE]
where Cyear &gt;=
(
	select cast(SET_VALUE as int)-1
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
and Fyear &lt;=
(
	select cast(SET_VALUE as int)+1
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
union all
--Tre anni a dettaglio mensile (da X+2 a X+7)
select 
MIN(Period_ID) as Period_ID,
'FY'+right(Fyear,2)+'-M'+right(FiscalMonth,2)+'-'+'CY'+RIGHT(cast(Cyear as char(4)),2) +'-'+cast(CivilMonth as char(4))
as Period,
min(PeriodStart) as PeriodStartDate,
max(PeriodEnd) as PeriodEndDate,
min(FiscalWeek) as FiscalWeek,
min(FiscalMonth) as FiscalMonth,
min(Fyear) as FiscalYear,
min(Fweek) as Fweek,
min(Fmonth) as Fmonth,
min(CivilMonth) as CivilMonth,
min(CYear) as CivilYear,
min(Cmonth) as Cmonth,
cast(cast(min(CYear) as char(4))+right(cast(min(CivilMonth) as char(4)),2) as int) as CivilMonthYear_ID,
cast(cast(min(Fyear) as char(4))+right(cast(min(FiscalWeek) as char(3)),2) as int) as FiscalWeekYear_ID
from SQL02.[SKY_SA].dbo.[M_PERIOD_SOURCE]
where Fyear &gt;=
(
	select cast(SET_VALUE as int)+2
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
and Fyear &lt;=
(
	select cast(SET_VALUE as int)+7
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
group by 
	'FY'+right(Fyear,2)+'-M'+right(FiscalMonth,2)+'-'+'CY'+RIGHT(cast(Cyear as char(4)),2) +'-'+cast(CivilMonth as char(4))
	,Fmonth
union all
--Tre anni a dettaglio annuale (da X+8 a X+10)
select 
MIN(Period_ID) as Period_ID,
'FY'+right(Fyear,2)+'-'+'CY'+RIGHT(cast(Cyear as char(4)),2)
as Period,
min(PeriodStart) as PeriodStartDate,
max(PeriodEnd) as PeriodEndDate,
min(FiscalWeek) as FiscalWeek,
min(FiscalMonth) as FiscalMonth,
min(Fyear) as FiscalYear,
min(Fweek) as Fweek,
min(Fmonth) as Fmonth,
min(CivilMonth) as CivilMonth,
min(CYear) as CivilYear,
min(Cmonth) as Cmonth,
cast(cast(min(CYear) as char(4))+right(cast(min(CivilMonth) as char(4)),2) as int) as CivilMonthYear_ID,
cast(cast(min(Fyear) as char(4))+right(cast(min(FiscalWeek) as char(3)),2) as int) as FiscalWeekYear_ID
from SQL02.[SKY_SA].dbo.[M_PERIOD_SOURCE]
where Fyear &gt;=
(
	select cast(SET_VALUE as int)+8
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
and Fyear &lt;=
(
	select cast(SET_VALUE as int)+10
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
group by 
'FY'+right(Fyear,2)+'-'+'CY'+RIGHT(cast(Cyear as char(4)),2), Fyear
union all
-- LAST Period (&lt;= X+11)
select 
MIN(Period_ID) as Period_ID,
'LAST-PERIOD' as Period,
min(PeriodStart) as PeriodStartDate,
max(PeriodEnd) as PeriodEndDate,
min(FiscalWeek) as FiscalWeek,
min(FiscalMonth) as FiscalMonth,
min(Fyear) as FiscalYear,
min(Fweek) as Fweek,
min(Fmonth) as Fmonth,
min(CivilMonth) as CivilMonth,
min(CYear) as CivilYear,
min(Cmonth) as Cmonth,
cast(cast(min(CYear) as char(4))+right(cast(min(CivilMonth) as char(4)),2) as int) as CivilMonthYear_ID,
cast(cast(min(Fyear) as char(4))+right(cast(min(FiscalWeek) as char(3)),2) as int) as FiscalWeekYear_ID
from SQL02.[SKY_SA].dbo.[M_PERIOD_SOURCE]
where Fyear &gt;=
(
	select cast(SET_VALUE as int)+11
	from M_SETTINGS
	where SET_KEY='CURRENT_FY_CREATION_PERIOD'
)
order by PeriodStartDate</property>
