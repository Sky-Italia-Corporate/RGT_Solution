      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{D33B4621-0AEC-4EB5-A704-F70CCF630888}"
          SQLTask:SqlStatementSource="-- DIMENSIONE FISCALE&#xA;IF  EXISTS (SELECT * FROM sys.objects &#xA;WHERE object_id = OBJECT_ID(N'[dbo].[S_PER_FISCALWEEK]') AND type in (N'U'))&#xA;DROP TABLE [dbo].[S_PER_FISCALWEEK]&#xA;&#xA;select distinct &#xA;FiscalWeekYear_ID,&#xA;'FY'+right(cast(FiscalYear as char(4)),2)+'-'+FiscalWeek as FiscalWeekYear,&#xA;cast(cast(FiscalYear as char(4))+right(cast(FiscalMonth as char(4)),2) as int) as FiscalMonthYear_ID&#xA;into S_PER_FISCALWEEK&#xA;from S_PER_PERIOD &#xA;order by FiscalWeekYear&#xA;&#xA;IF  EXISTS (SELECT * FROM sys.objects &#xA;WHERE object_id = OBJECT_ID(N'[dbo].[S_PER_FISCALMONTH]') AND type in (N'U'))&#xA;DROP TABLE [dbo].[S_PER_FISCALMONTH]&#xA;&#xA;select distinct &#xA;cast(cast(FiscalYear as char(4))+right(cast(FiscalMonth as char(4)),2) as int) as FiscalMonthYear_ID,&#xA;'FY'+right(cast(FiscalYear as char(4)),2)+'-'+FiscalMonth as FiscalMonthYear,&#xA;FiscalYear&#xA;into S_PER_FISCALMONTH&#xA;from S_PER_PERIOD &#xA;order by FiscalYear&#xA;&#xA;-- DIMENSIONE CIVILISTICA&#xA;IF  EXISTS (SELECT * FROM sys.objects &#xA;WHERE object_id = OBJECT_ID(N'[dbo].[S_PER_CIVILMONTH]') AND type in (N'U'))&#xA;DROP TABLE [dbo].[S_PER_CIVILMONTH]&#xA;&#xA;select distinct &#xA;cast(cast(CivilYear as char(4))+right(cast(CivilMonth as char(4)),2) as int) as CivilMonthYear_ID,&#xA;'CY'+RIGHT(cast(CivilYear as char(4)),2)+'-'+CivilMonth as CivilMonthYear,&#xA;CivilYear&#xA;into S_PER_CIVILMONTH&#xA;from S_PER_PERIOD &#xA;order by CivilYear" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
